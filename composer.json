{
    "name": "attla/core",
    "description": "The core of Attla Framework.",
    "keywords": [
        "attla",
        "framework",
        "core",
        "php"
    ],
    "homepage": "https://octha.com/attla",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "attla/laravel-html-minify": "dev-main",
        "doctrine/dbal": "^2.0 || ^3.0",
        "illuminate/cache": "^9.0",
        "illuminate/config": "^9.0",
        "illuminate/console": "^9.0",
        "illuminate/cookie": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/mail": "^9.0",
        "illuminate/log": "^9.0",
        "illuminate/pagination": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/translation": "^9.0",
        "illuminate/validation": "^9.0",
        "illuminate/view": "^9.0",
        "league/flysystem": "^3.0",
        "spatie/ignition": "^1.0",
        "voku/anti-xss": "^4.1",
        "vlucas/phpdotenv": "^5.4.1",
        "hisorange/browser-detect": "dev-stable"
    },
    "require-dev": {},
    "autoload": {
        "files": [
            "src/helpers/functions.php"
        ],
        "psr-4": {
            "Attla\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
